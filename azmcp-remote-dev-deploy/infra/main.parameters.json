{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "entraAppDisplayName": {
      "value": "Azure MCP Remote Server API"
    },
    "acaName": {
      "value": "azure-mcp-remote-server"
    },
    "appInsightsConnectionString": {
      "value": "DISABLED"
    },
    "outgoingAuthStrategy": {
      "value": "UseHostingEnvironmentIdentity"
    },
    "buildConfiguration": {
      "value": "Release"
    },
    "namespaces": {
      "value": ["storage"]
    }
  }
}